# Yandex API Gateway URL
# Получите этот URL после создания API Gateway в Yandex Cloud
# Формат: https://d5d...apigw.yandexcloud.net (БЕЗ слэша в конце!)
VITE_API_GATEWAY_URL=https://ваш-домен.apigw.yandexcloud.net

# Firebase конфигурация (уже настроено)
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-auth-domain
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-storage-bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# EmailJS (уже настроено)
VITE_EMAILJS_SERVICE_ID=your-service-id
VITE_EMAILJS_TEMPLATE_ID=your-template-id
VITE_EMAILJS_PUBLIC_KEY=your-public-key

# ImgBB API (для загрузки изображений)
VITE_IMGBB_API_KEY=your-imgbb-api-key

# OneSignal (для push уведомлений)
VITE_ONESIGNAL_APP_ID=your-onesignal-app-id

# Resend API (для email уведомлений - серверная)
RESEND_API_KEY=your-resend-api-key

# OneSignal REST API (серверная)
ONESIGNAL_REST_API_KEY=your-onesignal-rest-api-key
